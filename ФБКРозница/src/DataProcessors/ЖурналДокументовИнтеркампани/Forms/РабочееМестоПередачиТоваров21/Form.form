    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
	КонечныеОстатки.АналитикаУчетаНоменклатуры КАК АналитикаУчетаНоменклатуры,
	КонечныеОстатки.ВидЗапасовПродавца         КАК ВидЗапасовПродавца,
	КонечныеОстатки.НомерГТД                   КАК НомерГТД,
	КонечныеОстатки.КоличествоОстаток          КАК КоличествоОстаток,
	КонечныеОстатки.ВозвращеноОстаток          КАК ВозвращеноОстаток,
	КонечныеОстатки.ОрганизацияВладелец        КАК ОрганизацияВладелец
ПОМЕСТИТЬ КонечныеОстатки
ИЗ
	РегистрНакопления.ТоварыОрганизацийКПередаче.Остатки() КАК КонечныеОстатки
ГДЕ
	КонечныеОстатки.КоличествоОстаток > 0
	ИЛИ КонечныеОстатки.ВозвращеноОстаток > 0
ИНДЕКСИРОВАТЬ ПО
	АналитикаУчетаНоменклатуры,
	ВидЗапасовПродавца,
	НомерГТД
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ЗаПериод.ПериодМесяц                           КАК ПериодМесяц,
	ЗаПериод.ВидЗапасовПродавца.НалогообложениеНДС КАК ПередачаПодДеятельность,
	ЗаПериод.ОрганизацияВладелец                   КАК ОрганизацияВладелец,
	ЗаПериод.АналитикаУчетаНоменклатуры            КАК АналитикаУчетаНоменклатуры,
	ЗаПериод.ВидЗапасовПродавца                    КАК ВидЗапасовПродавца,
	ЗаПериод.НомерГТД                              КАК НомерГТД,
	ЗаПериод.КоличествоОборот                      КАК КоличествоОборот,
	ЗаПериод.ВозвращеноОборот                      КАК ВозвращеноОборот
ПОМЕСТИТЬ ЗаПериод
ИЗ
	РегистрНакопления.ТоварыОрганизацийКПередаче.ОстаткиИОбороты(&amp;НачГраница, &amp;КонГраница, Авто) КАК ЗаПериод
ГДЕ
	ЗаПериод.КоличествоОборот > 0
	ИЛИ ЗаПериод.ВозвращеноОборот &lt;> 0
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗЛИЧНЫЕ
	ЗаПериод.ПериодМесяц                           КАК Месяц,
	ВЫБОР
		КОГДА ВидЗапасов.ТипЗапасов = ЗНАЧЕНИЕ(Перечисление.ТипыЗапасов.КомиссионныйТовар)
			ТОГДА ЗНАЧЕНИЕ(Перечисление.ХозяйственныеОперации.ПередачаНаКомиссиюВДругуюОрганизацию)
		ИНАЧЕ ЗНАЧЕНИЕ(Перечисление.ХозяйственныеОперации.РеализацияТоваровВДругуюОрганизацию)
	КОНЕЦ                                          КАК ХозяйственнаяОперация,
	ЗаПериод.ВидЗапасовПродавца.НалогообложениеНДС КАК ПередачаПодДеятельность,
	СпрСклады.Ссылка                               КАК Склад,
	ЗаПериод.ОрганизацияВладелец                   КАК Отправитель,
	ВидЗапасов.Организация                         КАК Получатель,
	ВидЗапасов.ТипЗапасов                          КАК ТипЗапасов,
	ЗначенияПоУмолчанию.Договор                    КАК Договор,
	ЗначенияПоУмолчанию.ВидЦены                    КАК ВидЦены,
	ЗначенияПоУмолчанию.Валюта                     КАК ВалютаВзаиморасчетов
ИЗ
	ЗаПериод КАК ЗаПериод
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ КонечныеОстатки КАК КонечныеОстатки
		ПО ЗаПериод.ОрганизацияВладелец = КонечныеОстатки.ОрганизацияВладелец
			И ЗаПериод.АналитикаУчетаНоменклатуры = КонечныеОстатки.АналитикаУчетаНоменклатуры
			И ЗаПериод.ВидЗапасовПродавца = КонечныеОстатки.ВидЗапасовПродавца
			И ЗаПериод.НомерГТД = КонечныеОстатки.НомерГТД
	
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Справочник.ВидыЗапасов КАК ВидЗапасов
		ПО ЗаПериод.ВидЗапасовПродавца = ВидЗапасов.Ссылка
	
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.АналитикаУчетаНоменклатуры КАК Аналитика
		ПО Аналитика.КлючАналитики = ЗаПериод.АналитикаУчетаНоменклатуры
	
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Справочник.Склады КАК СпрСклады
		ПО Аналитика.Склад = СпрСклады.Ссылка
	
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.НастройкаПередачиТоваровМеждуОрганизациями КАК ЗначенияПоУмолчанию
		ПО ЗаПериод.ОрганизацияВладелец = ЗначенияПоУмолчанию.ОрганизацияВладелец
			И ВидЗапасов.ВидЗапасовВладельца.ТипЗапасов = ЗначенияПоУмолчанию.ТипЗапасов
			И ВидЗапасов.Организация = ЗначенияПоУмолчанию.ОрганизацияПродавец
	
ГДЕ
	(&amp;Получатель В (ЗНАЧЕНИЕ(Справочник.Организации.ПустаяСсылка), ВидЗапасов.Организация)
		ИЛИ &amp;Отправитель В (ЗНАЧЕНИЕ(Справочник.Организации.ПустаяСсылка), ЗаПериод.ОрганизацияВладелец))
	И ЗаПериод.КоличествоОборот &lt;> 0
	
ОБЪЕДИНИТЬ ВСЕ
	
ВЫБРАТЬ РАЗЛИЧНЫЕ
	ЗаПериод.ПериодМесяц                           КАК Месяц,
	ВЫБОР
		КОГДА ВидЗапасов.ТипЗапасов = ЗНАЧЕНИЕ(Перечисление.ТипыЗапасов.КомиссионныйТовар)
			ТОГДА ЗНАЧЕНИЕ(Перечисление.ХозяйственныеОперации.ВозвратПоКомиссииМеждуОрганизациями)
		ИНАЧЕ ЗНАЧЕНИЕ(Перечисление.ХозяйственныеОперации.ВозвратТоваровМеждуОрганизациями)
	КОНЕЦ                                          КАК ХозяйственнаяОперация,
	ЗаПериод.ВидЗапасовПродавца.НалогообложениеНДС КАК ПередачаПодДеятельность,
	СпрСклады.Ссылка                               КАК Склад,
	ВидЗапасов.Организация                         КАК Отправитель,
	ЗаПериод.ОрганизацияВладелец                   КАК Получатель,
	ВидЗапасов.ТипЗапасов                          КАК ТипЗапасов,
	ЗначенияПоУмолчанию.Договор                    КАК Договор,
	ЗначенияПоУмолчанию.ВидЦены                    КАК ВидЦены,
	ЗначенияПоУмолчанию.Валюта                     КАК ВалютаВзаиморасчетов
ИЗ
	ЗаПериод КАК ЗаПериод
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ КонечныеОстатки КАК КонечныеОстатки
		ПО ЗаПериод.ОрганизацияВладелец = КонечныеОстатки.ОрганизацияВладелец
			И ЗаПериод.АналитикаУчетаНоменклатуры = КонечныеОстатки.АналитикаУчетаНоменклатуры
			И ЗаПериод.ВидЗапасовПродавца = КонечныеОстатки.ВидЗапасовПродавца
			И ЗаПериод.НомерГТД = КонечныеОстатки.НомерГТД
	
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Справочник.ВидыЗапасов КАК ВидЗапасов
		ПО ЗаПериод.ВидЗапасовПродавца = ВидЗапасов.Ссылка
	
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.АналитикаУчетаНоменклатуры КАК Аналитика
		ПО Аналитика.КлючАналитики = ЗаПериод.АналитикаУчетаНоменклатуры
	
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Справочник.Склады КАК СпрСклады
		ПО Аналитика.Склад = СпрСклады.Ссылка
	
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.НастройкаПередачиТоваровМеждуОрганизациями КАК ЗначенияПоУмолчанию
		ПО ЗаПериод.ОрганизацияВладелец = ЗначенияПоУмолчанию.ОрганизацияВладелец
			И ВидЗапасов.ВидЗапасовВладельца.ТипЗапасов = ЗначенияПоУмолчанию.ТипЗапасов
			И ВидЗапасов.Организация = ЗначенияПоУмолчанию.ОрганизацияПродавец
	
ГДЕ
	(&amp;Отправитель В (ЗНАЧЕНИЕ(Справочник.Организации.ПустаяСсылка), ВидЗапасов.Организация)
		ИЛИ &amp;Получатель В (ЗНАЧЕНИЕ(Справочник.Организации.ПустаяСсылка), ЗаПериод.ОрганизацияВладелец))
	И ЗаПериод.ВозвращеноОборот &lt;> 0
